#include <stdio.h>
#include <locale.h>

main(){
	
	setlocale(LC_ALL, "portuguese");
	
	int a;
	
	printf("Digite o valor para saber se é maior, menor ou igual a 10: ");
	scanf("%d", &a);
	
	if(a>10){
		printf("%d é maior que 10",a);
	}else if(a<10){
		printf("%d é menor que 10",a);
	}else{
		printf("%d é igual a 10");
	}
}
